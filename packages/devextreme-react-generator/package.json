{
  "author": "Developer Express Inc.",
  "name": "devextreme-react-generator",
  "version": "1.1.0",
  "description": "DevExtreme React UI and Visualization Components",
  "repository": {
    "type": "git",
    "url": "https://github.com/DevExpress/devextreme-react.git"
  },
  "main": "dist/generator",
